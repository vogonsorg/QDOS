CC = gcc
# NOTE: -O2, -O3 and -Os are borked
CFLAGS = -O1 -Wall
CPPFLAGS =
#CPPFLAGS+= -DZDM
AR = ar
OBJS = au.o ac97_def.o dpmi_c.o mdma.o pcibios.o sc_cmi.o sc_e1371.o sc_ich.o sc_inthd.o sc_sbl24.o sc_sbliv.o sc_sbxfi.o sc_via82.o tim.o

libau.a:	$(OBJS)
	$(AR) rs libau.a $(OBJS)

all: libau.a

clean:
	rm -f *.o
distclean: clean
	rm -f *.a

%.o : %.c
	$(CC) $(CFLAGS) $(CPPFLAGS) -c $< -o $@
